<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Lock_Window__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
now()&gt;(ClosedDate + (($Setup.Hospitality_Case_Time_Window__c.Number_of_Hours__c * 60)/1440) ),
true,
false
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Case Lock Window</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
