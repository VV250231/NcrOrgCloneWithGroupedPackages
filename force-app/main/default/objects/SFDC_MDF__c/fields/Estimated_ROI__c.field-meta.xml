<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_ROI__c</fullName>
    <externalId>false</externalId>
    <formula>IF((Total_Estimated_Cost__c &gt; 0 &amp;&amp; ((Estimated_Revenue__c  * 0.25)&gt;=Total_Estimated_Cost__c)), (((Estimated_Revenue__c  * 0.25) -  Total_Estimated_Cost__c) / Total_Estimated_Cost__c), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Estimated ROI</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
