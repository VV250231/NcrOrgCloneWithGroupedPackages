<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_total_revenue_for_next_tier__c</fullName>
    <description>Next Tier Level Revenue - Total Solution Revenue</description>
    <externalId>false</externalId>
    <formula>IF(Revenue_to_Target__c &gt;  Total_Solution_Revenue__c , Revenue_to_Target__c - Total_Solution_Revenue__c , null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Revenue for Next tier level</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
