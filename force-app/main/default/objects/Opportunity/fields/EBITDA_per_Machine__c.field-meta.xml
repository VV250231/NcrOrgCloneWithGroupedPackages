<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EBITDA_per_Machine__c</fullName>
    <description>CATM - Ebitda per machine per month</description>
    <externalId>false</externalId>
    <formula>IF( Num_of_ATMs__c &gt; 0, 
  (( EBITA_Annualized__c /12)/ Num_of_ATMs__c), null)</formula>
