<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>valid_website</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(CONTAINS(Website, &quot;.&quot;)))</errorConditionFormula>
    <errorDisplayField>Website</errorDisplayField>
    <errorMessage>Please Enter the Valid Website Name</errorMessage>
</ValidationRule>
