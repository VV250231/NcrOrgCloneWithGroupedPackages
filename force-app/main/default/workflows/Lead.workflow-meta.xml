        <formula>AND(BEGINS(OwnerId , &quot;00G&quot;),RecordType.Name=&quot;Partner Community Lead&quot;, NOT($Setup.Automation_Setting__c.Skip_Validation_Rule__c), CATM_Record__c = false)</formula>
