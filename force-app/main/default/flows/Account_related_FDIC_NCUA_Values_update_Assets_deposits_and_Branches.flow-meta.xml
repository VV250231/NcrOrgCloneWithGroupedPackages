<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>52.0</apiVersion>
    <decisions>
        <description>In Account record we first Identify the field is FDIC,NCUA</description>
        <name>Field_Type_FDIC_OR_NCUA</name>
        <label>Field Type FDIC OR NCUA</label>
        <locationX>168</locationX>
        <locationY>351</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>FDIC</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.FDIC__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_FDIC_Records</targetReference>
            </connector>
            <label>FDIC</label>
        </rules>
        <rules>
            <name>NCUA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.NCUA__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_The_NCUA_Records</targetReference>
            </connector>
            <label>NCUA</label>
        </rules>
    </decisions>
    <description>Based on the FDIC,NCUA values in the Account the related Total Assets, Total Deposits and Number of the Branches will be updated based on the Institution&apos;s object</description>
    <environments>Default</environments>
    <interviewLabel>Account related FDIC NCUA Values update(Assets deposits and Branches) {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CATM Account related FDIC NCUA Values update(Assets deposits and Branches)</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Get the FDIC records from Institutions object record type is FDIC only</description>
        <name>Get_FDIC_Records</name>
        <label>Get FDIC Records</label>
        <locationX>385</locationX>
        <locationY>244</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Account_s_Total_Assets_deposits_and_Number_of_Branches</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>FDIC_Certificate__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.FDIC__c</elementReference>
            </value>
        </filters>
        <object>Institutions__c</object>
        <outputAssignments>
            <assignToReference>RecordID</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>NumberOFBreanches</assignToReference>
            <field>Number_of_Domestic_Offices__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>TotalAssets</assignToReference>
            <field>Total_Assets_formula__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>TotalDeposits</assignToReference>
            <field>Total_Deposits_formula__c</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Get_The_NCUA_Records</name>
        <label>Get The NCUA Records</label>
        <locationX>380</locationX>
        <locationY>437</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Account_record_Total_Assets_Total_Deposits_of_Brencahes</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>NCUA_Certificate__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.NCUA__c</elementReference>
            </value>
        </filters>
        <object>Institutions__c</object>
        <outputAssignments>
            <assignToReference>RecordID</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>TotalAssetsNCUA</assignToReference>
            <field>Total_Assets__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>TotalDepositsNCUA</assignToReference>
            <field>Total_Deposits__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>NumberofBranchesInNCUA</assignToReference>
            <field>of_Branches__c</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <name>Update_Account_record_Total_Assets_Total_Deposits_of_Brencahes</name>
        <label>Update Account record Total Assets,Total Deposits,#of Bernaches</label>
        <locationX>651</locationX>
        <locationY>438</locationY>
        <inputAssignments>
            <field>Branches__c</field>
            <value>
                <elementReference>NumberofBranchesInNCUA</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Institution_Name__c</field>
            <value>
                <elementReference>RecordID</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Assets__c</field>
            <value>
                <elementReference>TotalAssetsNCUA</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Deposits__c</field>
            <value>
                <elementReference>TotalDepositsNCUA</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Account_s_Total_Assets_deposits_and_Number_of_Branches</name>
        <label>Update Account&apos;s Total Assets deposits and Number of Branches</label>
        <locationX>643</locationX>
        <locationY>246</locationY>
        <inputAssignments>
            <field>Branches__c</field>
            <value>
                <elementReference>NumberOFBreanches</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Institution_Name__c</field>
            <value>
                <elementReference>RecordID</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Assets__c</field>
            <value>
                <elementReference>TotalAssets</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Deposits__c</field>
            <value>
                <elementReference>TotalDeposits</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>49</locationY>
        <connector>
            <targetReference>Field_Type_FDIC_OR_NCUA</targetReference>
        </connector>
        <filterFormula>AND(
NOT({!$Setup.Automation_Setting__c.Skip_Flow__c}),
OR(
NOT(ISBLANK({!$Record.FDIC__c})),
NOT(ISBLANK({!$Record.NCUA__c}))
)
)</filterFormula>
        <object>Account</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
