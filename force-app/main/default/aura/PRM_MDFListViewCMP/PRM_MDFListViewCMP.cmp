<aura:component controller="MDFListViewCmpController" implements="forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" >
    
    <aura:attribute name="mdfDetailRecords" type="MDFDetails[]"/>
    <aura:handler name="init" value="{!this}" action="{!c.getMDFRequest}" />
    <aura:attribute name="conditon" type="String"/>
    <aura:attribute name="offset" type="integer" />
    <aura:attribute name="next" type="boolean" />
    <aura:attribute name="prev" type="boolean" />
    <aura:attribute name="mdfRequest" type="SFDC_MDF__c" />
    <aura:attribute name="mdfClaim" type="SFDC_MDF_Claim__c" />
    <aura:attribute name="expenseList" type="MDF_Expense_Detail__c[]" />
    <aura:attribute name="files" type="File__c[]" />
    <aura:attribute name="filesForClaimDocs" type="File__c[]" />
    <aura:attribute name="recId" type="String"/>
    <aura:attribute name="strBack" type="String" default="none"/> 
    
    <aura:attribute name="perApprovedDocs" type="String[]" /> 
    <aura:attribute name="proofOfPerformanceDocs" type="String[]" /> 
    <aura:attribute name="strDescription" type="String" />
    <aura:attribute name="fileRecordId" type="String" />
    
    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:attribute name="isClone" type="boolean" default="false"/>
    <aura:attribute name="isEdit" type="boolean" default="false"/>
    <aura:attribute name="isClaim" type="boolean" default="false"/>
    <aura:attribute name="isPreApprovalModal" type="boolean" default="false"/>
    <aura:attribute name="isProofOfPerformanceModal" type="boolean" default="false"/>
    <aura:attribute name="isNewClaimDoc" type="boolean" default="true"/>
    <aura:attribute name="isFromPreApproval" type="boolean" default="false"/>
    <aura:attribute name="isEditSupportingDoc" type="boolean" default="false"/>
    <aura:attribute name="isActivityResult" type="boolean" default="false"/>
    <aura:attribute name="isActivityResultError" type="boolean" default="false"/>
    <aura:attribute name="isDeleteConfirm" type="boolean" default="false"/>
    
    <aura:attribute name="numOfPreApprovalDoc" type="Integer" default="0" />
    <aura:attribute name="numOfPOPDoc" type="Integer" default="0"/>
    
    <!-- 'showLoadingSpinner' attribute for show/hide the uploading image and msg in aura:if-->
  	<aura:attribute name="showLoadingSpinner" type="boolean" default="false" />
 	<!-- 'fileName' attribute for display the selected file name -->  
   	<aura:attribute name="fileName" type="String" default="No File Selected.." />
    
    <aura:handler name="CancelRequestEvt" event="c:CancelMDFRequestEvt" action="{!c.cancelRequest}"/>
    <aura:handler name="DetailRequestEvt" event="c:DetailMDFRequestEvt" action="{!c.detailMDFRequest}"/>
	<aura:handler name="CloneCancelRequestEvt" event="c:CancelMDFRequestEvt" action="{!c.cloneCancelRequest}"/>
    
    <aura:handler event="aura:waiting" action="{!c.showSpinner}"/>
    <aura:handler event="aura:doneWaiting" action="{!c.hideSpinner}"/>
    
    <aura:attribute name="Spinner" type="boolean" default="false"/>
    
    <aura:attribute name="isMDFRequestApproval" type="boolean" default="false" />
    <aura:attribute name="mdfReqeustApprovalComment" type="String" />
    
    <aura:attribute name="isMDFClaimApproval" type="boolean" default="false" />
    <aura:attribute name="mdfClaimApprovalComment" type="String" />
    
     <aura:attribute name="isListView" type="boolean" default="true"/>
    <aura:attribute name="isDetailView" type="boolean" default="false"/>
    
	<!--loading spinner start... style=Brand Medium (blue dots)-->
    <aura:if isTrue="{!v.Spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
           <div class="slds-spinner_brand  slds-spinner slds-spinner_large slds-is-relative" role="alert">
             <span class="slds-assistive-text">Loading</span>
             <div class="slds-spinner__dot-a"></div>
             <div class="slds-spinner__dot-b"></div>
           </div>
        </div>
     </aura:if>
    <!-- Loading spinner end--> 
    <aura:if isTrue="{!v.isListView}">
    <div class="slds-show" aura:id="ListView">
        <div class="slds-page-header" role="banner">
            <div class="slds-grid slds-wrap">
                <div class="slds-col slds-has-flexi-truncate"> 
                    <div class="slds-media slds-media_center slds-no-space slds-grow">
                        <div class="slds-media__figure">
                            <span class="slds-icon_container slds-icon-standard-account">
                                <lightning:icon iconName="standard:account" size="large" alternativeText="Indicates approval"/>
                                <span class="slds-assistive-text">MDF Request</span>
                            </span>
                        </div>
                        <div class="slds-media__body slds-size_1-of-4 ">
                            <p class="slds-text-title_caps slds-line-height_reset" title="MDF Request">MDF Requests</p>
                            <ui:inputSelect aura:id="levels" class="slds-m-right_small slds-align-middle slds-size_1-of-4 dropdownHeader"  change="{!c.onSelectChange}">
                                <ui:inputSelectOption text="All" label="All" value="true"/>
                                <ui:inputSelectOption text="Created Today" label="Created Today"/>
                                <ui:inputSelectOption text="Pending" label="Pending"/>
                                <ui:inputSelectOption text="Approved" label="Approved"/>
                                <ui:inputSelectOption text="Rejected" label="Rejected"/>
                                <ui:inputSelectOption text="Request Pending for Approvals" label="Request Pending for Approvals"/>
                            </ui:inputSelect>
                        </div>
                    </div>                    
                </div>  
                <!-- ACTION BUTTONS -->
                <div class="slds-col slds-no-flex slds-grid slds-wrap slds-align-top">
                    <div class="slds-button-group" role="group">
                        <button class="slds-button slds-button_neutral" onclick="{!c.showNewRequest}">
                            New MDF Request
                        </button>
                    </div>
                </div>
                <!-- / ACTION BUTTONS -->               
            </div>
        </div>
        
        
        <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal"><!--Table must be responsive-->
            <thead>
                <tr class="slds-text-heading_label ">
                    <th class="" scope="col">Action</th>                    
                    <th class="" scope="col">Activity</th>                    
                    <th class="slds-is-sortable" scope="col">MDF Request Status</th>
                    <th class="slds-is-sortable" scope="col">Activity Start Date</th>
                    <th class="slds-is-sortable" scope="col">Claim Status</th>
                    <th class="slds-is-sortable" scope="col">Request No</th>           
                </tr>  
            </thead>
            <tbody>
                <aura:iteration items="{!v.mdfDetailRecords}" var="mdf">
                    <tr class="slds-hint-parent">  
                        <td data-label="Action">
                            <aura:if isTrue="{!mdf.objFundRequest.Status__c == 'Request Created'}">
                                <a data-ids="{!mdf.objFundRequest.Id}" onclick="{!c.deleteMdf}"> Del </a>
                            </aura:if> 
                        </td>
                        <td data-label="Activity" ><a data-record="{!mdf.objFundRequest.Id}" onclick="{!c.redirectToMdfRecord}">
                            {!mdf.objFundRequest.Name}</a>
                        </td>
                        <td data-label="MDF Request Status" ><ui:outputText value="{!mdf.objFundRequest.Status__c}" /></td>
                        <td data-label="Activity Start Date" >{!mdf.objFundRequest.Activity_Date__c}</td>
                        <td data-label="Claim Status"> {!mdf.claimStatus} </td>
                        <td data-label="Request No">{!mdf.objFundRequest.Request_ID__c}</td> 
                    </tr>
                </aura:iteration>
            </tbody>
        </table>
         <ui:button class="slds-button slds-button_neutral slds-p-horizontal_xx-small slds-m-right_x-small slds-float_left" press="{!c.Previous}" disabled="{!v.prev}">
            <span class="slds-icon slds-icon-text-default">
                <lightning:icon iconName="utility:chevronleft" size="small" alternativeText="Indicates approval"/>
                <span class="slds-assistive-text">Previous</span>
            </span>               
        </ui:button> 
        <ui:button class="slds-button slds-button_neutral slds-p-horizontal_xx-small slds-m-right_x-small slds-float_left" press="{!c.Next}" disabled="{!v.next}" >
            <span class="slds-icon slds-icon-text-default">
                <lightning:icon iconName="utility:chevronright" size="small" alternativeText="Indicates approval"/>
                <span class="slds-assistive-text">Next</span>
            </span>                   
        </ui:button>
       
    </div>
    </aura:if>
    <!---New MDF Request Modal window -->
	<aura:if isTrue="{!v.isOpen}">
       <c:PRM_MDFNewRecordCMP />
    </aura:if>
    
    <!---Clone MDF Request Modal window -->
	<aura:if isTrue="{!v.isClone}">
       <c:PRM_MDFCloneMDFRequest mdfRequestId="{!v.recId}"/>
    </aura:if>
    
    <!---New MDF Request Modal window -->
	<aura:if isTrue="{!v.isEdit}">
       <c:PRM_MDFEditRequest mdfRequestId="{!v.recId}"/>
    </aura:if>
    
     <!---New MDF Request Modal window -->
	<aura:if isTrue="{!v.isClaim}">
       <c:PRM_CreateMDFClaim mdfRequestId="{!v.recId}"/>
    </aura:if>
    
    <!--###### MODAL BOX Part END Here ######-->
    
    <!---Redirecting To Show Fields -->
    <aura:if isTrue="{!v.isDetailView}">
    <div class="slds-scope slds-grid slds-show" aura:id = "OutputArea">
        <!-- activity details -->
        <div class="slds-m-top_small slds-text-align_center">
             <aura:if isTrue="{! and(v.mdfRequest.Status__c == 'Request Created', v.mdfRequest.Is_Record_Locked__c == false)}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.showEditMDFRequest}">Edit</button>
            </aura:if>
            <aura:if isTrue="{! and(v.mdfRequest.Status__c == 'Request Created', v.mdfRequest.Is_Record_Locked__c == false)}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.showMDFRequestModal}">Submit MDF Request</button>
            </aura:if>
            
            <aura:if isTrue="{!or(v.mdfRequest.Status__c == 'Request Created' , v.mdfRequest.Status__c == 'Rejected')}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.deleteMDFDetail}">Delete</button>
            </aura:if>
             
             <aura:if isTrue="{!and(v.mdfRequest.Status__c == 'Approved', v.mdfClaim == null)}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.callCreateClaim}">Create Claim</button>
            </aura:if>
            
             <aura:if isTrue="{!and(and(v.mdfRequest.Status__c == 'Approved', v.mdfClaim != null), v.mdfClaim.Status__c =='Created')}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.showMDFClaimModal}">Submit Claim</button>
            </aura:if>
            
            <button class="slds-button slds-button_neutral" type="button" onclick="{!c.openCloneMDF}">Clone</button>
            <aura:if isTrue="{!v.mdfRequest.Status__c == 'Approved'}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.openActivityResult}">Activity Result</button>
            </aura:if>
            
            <aura:if isTrue="{!v.strBack == 'none'}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.showListview}">Back</button>
            </aura:if>
             <aura:if isTrue="{!v.strBack != 'none'}">
            	<button class="slds-button slds-button_neutral" type="button" onclick="{!c.backOnListViewDetail}">Back</button>
            </aura:if>
            
        </div>
        <div class="section">
            <h2 class="slds-text-heading_small section-heading">Activity Profile</h2>
            <div>
                <div class="slds-grid slds-wrap section-body">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-col_padded">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity name</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Name}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity Type</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Activity_Type__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Status</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Status__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity Contact</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Requester_Name__r.Name}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Other Vendors Represented</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Other_Vendors_Represented__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity Description</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Activity_Description__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Transaction Reference</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfClaim.Transaction_Reference__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-col_padded">
                        <div class="slds-grid">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity Start Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Activity_Date__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Activity End Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.End_Date__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Request Submission Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputDateTime value="{! v.mdfRequest.Request_Submission_Date__c}" class="slds-form-element__static" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Approved Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Approved_Date__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- endof activity details -->

        <!-- claim details -->
        <aura:if isTrue="{! v.mdfClaim != null}">
        <div class="section">
            <h2 class="slds-text-heading_small section-heading">Claim Detail</h2>
            <div>
                <div class="slds-grid slds-wrap section-body">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-col_padded">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Status</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfClaim.Status__c}" class="slds-form-element__static"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-col_padded">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Claim Submission Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputDateTime value="{! v.mdfClaim.Claim_Submission_Date__c}" class="slds-form-element__static" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Approved Date</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputDate value="{! v.mdfClaim.Approved_Date__c}" class="slds-form-element__static" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>    
        </div>
        </aura:if>
        <!-- endof claim details -->


        <!-- Financial Profile -->
        <div class="section">
            <h2 class="slds-text-heading_small section-heading">Financial Profile</h2>
            <div>
                <div class="slds-grid slds-wrap section-body">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-col_padded">
                        <aura:if isTrue="{! or(v.mdfRequest.Activity_Type__c == 'Print - Collateral' , v.mdfRequest.Activity_Type__c == 'Print - Brochure')}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Estimated Number of Copies</span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Number_of_Copies__c}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        </aura:if>
                        
                        
                        <aura:if isTrue="{! or(v.mdfRequest.Activity_Type__c == 'Print - Publication' , v.mdfRequest.Activity_Type__c == 'Print - White Paper')}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Estimated Circulation </span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Circulation__c}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        </aura:if>   
                        <aura:if isTrue="{! v.mdfRequest.Activity_Type__c == 'E-marketing - Microsite'}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">	Estimated Number of Unique Site Visitors </span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Number_of_Unique_Site_Visitors__c}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        </aura:if>   
                        <aura:if isTrue="{! or(v.mdfRequest.Activity_Type__c == 'Sales Program - Telesales' , v.mdfRequest.Activity_Type__c == 'Sales Program - Incentive')}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Estimated Number of Sales </span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Number_of_Sales__c}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        </aura:if>   
                        <aura:if isTrue="{! or(v.mdfRequest.Activity_Type__c == 'Sales Program - Telesales', v.mdfRequest.Activity_Type__c == 'Sales Program - Telemarketing')}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Estimated Number of Calls </span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Number_of_Calls__c}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        </aura:if>   
                        <aura:if isTrue="{! or(or(v.mdfRequest.Activity_Type__c == 'Sales Program - Telemarketing' , v.mdfRequest.Activity_Type__c == 'E-marketing - Social Media' ), 
                                                or(or(v.mdfRequest.Activity_Type__c == 'E-marketing - Endorsed Outsourced Mktg' , v.mdfRequest.Activity_Type__c == 'E-marketing - Email') , 
                                                   or(v.mdfRequest.Activity_Type__c == 'Event - Roadshow' , v.mdfRequest.Activity_Type__c == 'Event - Tradeshow')))}">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1">
                                <div class="slds-form-element slds-hint-parent slds-border_bottom">
                                    <span class="slds-form-element__label">Estimated Number of Leads </span>
                                    <div class="slds-form-element__control">
                                        <ui:outputText value="{! v.mdfRequest.Number_of_Leads__c}"/>
