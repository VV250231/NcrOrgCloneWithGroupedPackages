<aura:component controller="ActiveContactController" implements="flexipage:availableForAllPageTypes,force:hasRecordId">
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="ContactId" type="Id"  default="{!v.recordId}"/>
    <aura:attribute name="contact" type="Contact"/>   
</aura:component>