        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>First Touch to Qualified (Hours)</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(Lead.SQL_Trigger__c:SUM=0,Lead.SQL_Trigger__c:SUM,Lead.SQL_Trigger__c:SUM/RowCount)</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA3</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Lead to Qualified Lead %</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(CONVERTED:SUM=0,CONVERTED:SUM,CONVERTED:SUM/Lead.SQL_Trigger__c:SUM)</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA4</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Qualified Lead to Opp %</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(WON:SUM=0,WON:SUM,WON:SUM/CONVERTED:SUM)</calculatedFormula>
